<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hp.assistent">
    <uses-permission android:name="android.permission.USE_FINGERPRINT"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Assistent"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddActivity" />
        <activity android:name=".ViewReminders" />
        <activity android:name=".EditList" />
        <activity android:name=".EditPage" />
        <activity android:name=".DeleteEvent" />
        <activity android:name=".FingerPrint"></activity>
    </application>

</manifest>